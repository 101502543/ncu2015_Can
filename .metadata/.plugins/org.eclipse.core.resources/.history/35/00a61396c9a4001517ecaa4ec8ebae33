package cot.qin.sre;

public class DOMmok {
	public static DOMmok domMok = null;
	private int X,Y;
	public static DOMmok getInstance() {
		if (domMok == null)
			domMok = new DOMmok();
		return domMok;
	}

	public int[] getVirtualCharacterXY() {
		return new int[] { X, Y };
	}
	private void setXY(int x,int y) {
		X = x;
		Y = y;
	}
}
